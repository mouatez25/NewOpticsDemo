<dx:DXWindow xmlns:dxco="http://schemas.devexpress.com/winfx/2008/xaml/controls"  x:Class="GestionClinique.Patient.VisiteAs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
     dx:ThemeManager.ThemeName="Office2010Black"
               xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"

             xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionClinique.Patient"
        mc:Ignorable="d"
        Title="Consultation assistée" Height="700" Width="500"  ResizeMode="CanResize" Icon="/Diabetosoft;component/Dent6.ico" WindowStartupLocation="CenterScreen"   BorderThickness="2" WindowStyle="SingleBorderWindow" Padding="0,0,0,0" Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}" Closing="DXWindow_Closing"  >
    <Grid x:Name="gridgeneral" Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" >
        <dxco:Wizard x:Name="VisteAssisteeWizard" Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}" Finish="VisteAssisteeWizard_Finish" BorderBrush="#FFB27300" >

            <dxco:WelcomeWizardPage Title="Consultation Clinique" Header="une évaluation de la charge ou surcharge pondérale">
                <Border x:Name="windowBorderExamenIMC"   BorderThickness="3" CornerRadius="20" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="10">
                    <Grid x:Name="txtIMDC" Margin="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>



                        </Grid.ColumnDefinitions>
                        <TextBlock x:Name="txtIMCLabel" HorizontalAlignment="Center" Text="Indice de Masse Corporelle :" VerticalAlignment="Center" Margin="5,5,0,0" FontFamily="Tahoma" TextDecorations="Underline" Foreground="#FFB27300" FontSize="14" FontWeight="Bold" />

                        <Label x:Name="TAILELABEL" FontFamily="Tahoma" FontSize="14" Content="Taille (m) :" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtTaile" Text="{Binding Taille, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" MinWidth="100" FontFamily="Tahoma" Margin="80,0,5,0" Grid.Row="1" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" HorizontalAlignment="Center" TextChanged="txtTaile_TextChanged" KeyDown="txtTaile_KeyDown" />
                        <Label x:Name="poidLABEL" FontFamily="Tahoma" FontSize="14" Content="Poids (kg):" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="2" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtPois" Text="{Binding Poids, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" MinWidth="100" FontFamily="Tahoma" Margin="80,0,5,0" Grid.Row="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" HorizontalAlignment="Center" KeyDown="txtPois_KeyDown" TextChanged="txtPois_TextChanged" SelectionBrush="#FFB27300" />
                        <Label x:Name="IMCLABEL" FontFamily="Tahoma" FontSize="14" Content="IMC :" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="3" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <Label x:Name="ValeurImc" Content="{Binding IMC, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" FontFamily="Tahoma" FontSize="24"  Margin="50,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" MinWidth="100" Grid.Row="3" Foreground="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}" FontWeight="Bold" />
                        <Label x:Name="graisse" FontFamily="Tahoma" FontSize="14" Content="Répartition des graisses :" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Center" Grid.Row="4" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtGraisse" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding GraisseRépartiton, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" MinWidth="190" FontFamily="Tahoma" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" Margin="5,30,5,5" MinHeight="72" Grid.Row="4" Grid.RowSpan="2" Grid.ColumnSpan="4" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />
                        <TextBlock x:Name="txtNoroloLabel" HorizontalAlignment="Center" Text="Examen neurologique :" VerticalAlignment="Center" Margin="5,5,0,0" FontFamily="Tahoma" TextDecorations="Underline" Grid.Row="6" Foreground="#FFB27300" FontSize="14" FontWeight="Bold" />
                        <Label x:Name="Norologi" FontFamily="Tahoma" FontSize="14" Content="pour rechercher:" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Center" Grid.Row="7" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" FontWeight="Bold" />

                        <TextBox x:Name="txtNorologique" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding NorologiqueExamen, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" FontFamily="Tahoma" Controls:TextBoxHelper.Watermark="Une polynévrite:fourmillement,démangeaisons,sensations de froid/chaud" Margin="5,30,5,5" Grid.Row="7" Grid.RowSpan="2" Grid.ColumnSpan="4" Controls:TextBoxHelper.UseFloatingWatermark="True" Foreground="#FFB27300" FontWeight="Bold" SelectionBrush="#FFB27300" />

                    </Grid>
                </Border>

            </dxco:WelcomeWizardPage>
            <dxco:WizardPage Title="Consultation Clinique" Header="Auscultation du cœur et des vaisseaux" Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}">
                <Border x:Name="windowBorderExamenCardio"   BorderThickness="3" CornerRadius="20" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" Grid.Column="3" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="10">
                    <Grid x:Name="txtCardiAdulte" Margin="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*" />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>


                        </Grid.ColumnDefinitions>
                        <TextBlock x:Name="txtExamenCardio" HorizontalAlignment="Center" Text="Examen cardio-vasculaire :" VerticalAlignment="Top" Margin="5,5,0,0" FontFamily="Tahoma" TextDecorations="Underline" Grid.ColumnSpan="2" FontWeight="Bold" FontSize="14" Foreground="#FFB27300" />

                        <Label x:Name="TensionABEL" FontFamily="Tahoma" FontSize="14" Content="Tension artérielle :" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtTensionArtérielle" Text="{Binding Tensionartérielle, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" MinWidth="60" FontFamily="Tahoma" Margin="140,0,5,0" Grid.Row="1" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" HorizontalAlignment="Center" KeyDown="txtTaile_KeyDown" SelectionBrush="#FFB27300" />
                        <Label x:Name="TensionOrthoABEL" FontFamily="Tahoma" FontSize="14" Content="Tension artérielle" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="28" Grid.Row="2" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <Label x:Name="TensionOrthoABELortho" FontFamily="Tahoma" FontSize="14" Content=" orthostatique :" Margin="5,15,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="28" Grid.Row="2" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />

                        <TextBox x:Name="txtTensionArtérielleOrth" Text="{Binding Tensionartérielleorthostatique, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" MinWidth="90" FontFamily="Tahoma" Margin="110,0,5,0" Grid.Row="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" HorizontalAlignment="Center" Width="80" KeyDown="txtTaile_KeyDown" SelectionBrush="#FFB27300" />

                        <Label x:Name="POULSLABEL" FontFamily="Tahoma" FontSize="14" Content="Pulsation :" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="3" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtPulsation" Text="{Binding Pulsation, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" MinWidth="90" FontFamily="Tahoma" Margin="110,-2,5,0"  Grid.Row="3" VerticalAlignment="Center" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" HorizontalAlignment="Center" KeyDown="txtTaile_KeyDown" SelectionBrush="#FFB27300" />

                        <Label x:Name="MCRLABEL" FontFamily="Tahoma" FontSize="14" Content="Macro-angiopathie :" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtMacroangiopathie" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding Macroangiopathie, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="190" FontFamily="Tahoma" Margin="5,30,5,5" MinHeight="72" Grid.Row="4" Grid.RowSpan="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />
                        <Label x:Name="soufflelabel" FontFamily="Tahoma" FontSize="14" Content="Souffles vasculaires :" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="5" Grid.Column="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtsoufflesvasculaire" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding Soufflesvasculaires, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="205" FontFamily="Tahoma" Margin="10,30,5,5" MinHeight="35" Grid.Row="5" Grid.Column="1" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />


                        <Label x:Name="InsuffisanceLABEL" FontFamily="Tahoma" FontSize="14" Content="Insuffisance cardiaque  :" Margin="10,0,0,0" Grid.Column="1" Grid.Row="1" VerticalAlignment="Top" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtInsuffisance" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding Insuffisancecardiaque, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="190" FontFamily="Tahoma" Margin="10,30,5,0"  Grid.Column="1" MinHeight="72" Grid.Row="1" Grid.RowSpan="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />
                        <Label x:Name="Oedemeslabel" FontFamily="Tahoma" FontSize="14" Content="Oedèmes des membres inférieurs :" Margin="0" VerticalAlignment="Top" Grid.Column="1" Grid.Row="3" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtDoeèmes" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding Oedèmesdesmembres, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="190" FontFamily="Tahoma" Margin="10,30,5,0"  Grid.Column="1" MinHeight="72" Grid.Row="3" Grid.RowSpan="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />
                    </Grid>
                </Border>

            </dxco:WizardPage>
            <dxco:WizardPage Title="Consultation Clinique" Header="Examen des yeux" Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}">
                <Border x:Name="windowBorderExamenExamen"   BorderThickness="3" CornerRadius="20" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="5,0,0,0">
                    <Grid x:Name="txtYeux" Margin="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*" />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>



                        </Grid.ColumnDefinitions>
                        <TextBlock x:Name="txtExamendeyeux" HorizontalAlignment="Center" Text="Examen rapides des yeux :" VerticalAlignment="Top" Margin="5,5,0,0" FontFamily="Tahoma" TextDecorations="Underline" Grid.ColumnSpan="2" FontSize="14" FontWeight="Bold" Foreground="#FFB27300" />
                        <Label x:Name="APPREVascu" FontFamily="Tahoma" FontSize="14" Content="* Une appréciation de l'état de la vascularisation  " Margin="20,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <Label x:Name="APPREVascu1" FontFamily="Tahoma" FontSize="14" Content="de la rétine" Margin="30,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Grid.Row="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtVasc" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding étatvascularisation, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="205" FontFamily="Tahoma" Margin="10,0,10,5" MinHeight="72" Grid.Row="2" Grid.RowSpan="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />
                        <Label x:Name="APPREVISU" FontFamily="Tahoma" FontSize="14" Content="* Une étude de l'acuité visuelle" Margin="20,0,5,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="4" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" />
                        <TextBox x:Name="txtVisu" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding Uneétudedelacuité, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="205" FontFamily="Tahoma" Margin="10,0,10,5" MinHeight="72" Grid.Row="5" Grid.RowSpan="2" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />

                    </Grid>
                </Border>

            </dxco:WizardPage>
            <dxco:WizardPage Title="Consultation Clinique" Header="Examen de la peau,pieds" Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}">
                <Border x:Name="windowBorderExamenPeau"   BorderThickness="3" CornerRadius="20" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" Grid.Column="3" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="5,0,0,0">
                    <Grid x:Name="txtCards" Margin="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*" />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>


                        </Grid.ColumnDefinitions>
                        <TextBlock x:Name="txtExamendelapeu" HorizontalAlignment="Center" Text="Examen de la peau,pieds :" VerticalAlignment="Top" Margin="5,5,0,0" FontFamily="Tahoma" TextDecorations="Underline" Grid.ColumnSpan="2" Foreground="#FFB27300" FontSize="14" FontWeight="Bold" />
                        <Label x:Name="PeauABEL" FontFamily="Tahoma" FontSize="14" Content="La peau pour rechercher:" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="1" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" FontWeight="Bold" />
                        <Label x:Name="PiedsABEL" FontFamily="Tahoma" FontSize="14" Content="Les pieds pour rechercher:" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" Grid.Column="1" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" FontWeight="Bold" />
                        <Label x:Name="FuronclesLabel" FontFamily="Tahoma" Content="* Furoncles" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />
                        <TextBlock x:Name="LésionLabel" TextWrapping="Wrap"  FontFamily="Tahoma" Text="* Lésions aux niveaux des narines,fesses,conduits,auditifs..." Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Grid.Row="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />
                        <Label x:Name="PlaiessLabel" FontFamily="Tahoma" Content="* Plaies" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="2" Grid.Column="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />
                        <Label x:Name="CallositésLabel" FontFamily="Tahoma" Content="* Callosités" Margin="0" HorizontalAlignment="Right" VerticalAlignment="Top" Grid.Row="2" Grid.Column="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />
                        <Label x:Name="InfectionsLabel" FontFamily="Tahoma" Content="* Infections des ongles" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Grid.Row="2" Grid.Column="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />
                        <Label x:Name="MycoLabel" FontFamily="Tahoma" Content="* Mycoses" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />
                        <Label x:Name="DéfLabel" FontFamily="Tahoma" Content="* Déformation du pied" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="2" Grid.Column="1" FontWeight="Bold" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" />


                        <TextBox x:Name="txtPeau" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding ExamenPeau, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="205" FontFamily="Tahoma" Margin="5,20,5,5" MinHeight="72" Grid.Row="2" Grid.RowSpan="4" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />

                        <TextBox x:Name="txtPieds" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" TextWrapping="Wrap" Text="{Binding ExamenPieds, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Controls:TextBoxHelper.Watermark="Champ max MAX caractére" MinWidth="205" FontFamily="Arial" Margin="5,0,5,5" MinHeight="72" Grid.Row="3" Grid.RowSpan="4" Grid.Column="1" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontWeight="Bold" SelectionBrush="#FFB27300" />





                    </Grid>
                </Border>

            </dxco:WizardPage>
            <dxco:CompletionWizardPage Title="Bilan biologique" Header="This is the last page" Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}">
                <Border x:Name="windowBorderparacliniques"   BorderThickness="3" CornerRadius="20" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" Grid.Column="3" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Margin="10">

                    <Grid x:Name="ConstaneAnamneseDétailGrid" IsEnabled="True"  Grid.RowSpan="8" Margin="10" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition   />
                            <RowDefinition  />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition  />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                            <RowDefinition   />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition />
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="Nouvelle Prise:" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#FFB27300" FontWeight="Bold" Margin="10,0,0,0" FontSize="18.667" FontFamily="Tahoma" />

                        <DatePicker x:Name="DatePriseAnamnese"  Grid.Column="1" VerticalAlignment="Center" FontFamily="Tahoma" FontWeight="Bold" FontSize="16" Margin="10,0,0,0" Height="28" BorderBrush="#FFB27300" />
                        <Border VerticalAlignment="Bottom" Grid.ColumnSpan="3" BorderBrush="{DynamicResource {x:Static SystemColors.ScrollBarBrushKey}}" BorderThickness="0,0,0,2" />
                        <WindowsFormsHost Grid.RowSpan="1" Grid.Column="2" Margin="10,0,5,0" VerticalAlignment="Center" Height="28" Width="105">
                            <wf:MaskedTextBox x:Name="txtTimeAnamnese" Size="15, 15" Text="00:00"  Mask="00:00" Dock="Fill"/>
                        </WindowsFormsHost>


                        <TextBlock Text="Poids" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" FontWeight="Bold" Margin="10,0,0,0" FontSize="18.667" FontFamily="Tahoma" Grid.Row="1" />
                        <TextBox x:Name="txtPoidAnamnese" Text="{Binding Poid, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" VerticalAlignment="Center" Grid.Column="1" Grid.Row="1" Margin="10,0,0,0" FontFamily="Tahoma" Foreground="#FFB27300" Controls:TextBoxHelper.UseFloatingWatermark="True" FontSize="14" FontWeight="Bold" KeyDown="txtPoid_KeyDown" TextChanged="txtPoidAnamnese_TextChanged" SelectionBrush="#FFB27300" />
                        <TextBlock Text="Kg" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" FontWeight="Bold" Margin="10,0,0,0" FontSize="18.667" FontFamily="Tahoma" Grid.Row="1" Grid.Column="2" />
                        <TextBlock Text="Taille" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" FontWeight="Bold" Margin="10,0,0,0" FontSize="18.667" FontFamily="Tahoma" Grid.Row="2" />
